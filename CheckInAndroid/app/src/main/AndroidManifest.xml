<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.saish1396.checkinandroid" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".welcomeOne"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mainScanOne"
            android:label="@string/title_activity_main_scan_one"
            android:parentActivityName=".welcomeOne" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="io.github.saish1396.checkinandroid.welcomeOne" />
        </activity>
    </application>

</manifest>
